# OpenRouter API Configuration
# Get your API key from: https://openrouter.ai/keys
# Don't forget to set a credit limit to avoid suprising bills!!!
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Encryption Secret for BYOK encryption
API_KEY_ENCRYPTION_SECRET=your-api-key-encryption-secret

# Model Providers
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_GENERATIVE_AI_API_KEY=your_google_generative_ai_api_key_here

# Supabase Configuration (if using Supabase for auth/database)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Database URL (for Prisma)
DATABASE_URL=your_database_connection_string
# with Supabase its recommended to use DIRECT_URL for migrations
DIRECT_URL=your-supabase-direct-url

# NODE ENV
NODE_ENV=development

# SEO
NEXT_PUBLIC_SITE_URL=your-production-url